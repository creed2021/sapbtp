     xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
    xmlns:smartList="sap.ui.comp.smartlist"
    xmlns:smartTable="sap.ui.comp.smarttable">
    <Page id="pageS" title="{i18n>title}">
    <content>
    <Text id="t20" text="hola Suppliers"></Text>
    <smartFilterBar:SmartFilterBar id="smartFilterBar2" persistencyKey="SmartFilter_Explored" entitySet="Products">
   </smartFilterBar:SmartFilterBar>
        
        <smartTable:SmartTable id="smarttable2" initiallyVisibleFields="SupplierID,CompanyName"
        
